    {
      "definition": {
        "viz": "timeseries",
        "requests": [
          {
            "q": "avg:mysql.performance.handler_read_key{$env,$customerid,$host} by {host}",
            "conditional_formats": [],
            "type": "area",
            "aggregator": "avg"
          }
        ]
      },
