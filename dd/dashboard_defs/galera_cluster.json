    },
    {
      "definition": {
        "viz": "timeseries",
        "requests": [
          {
            "q": "avg:mysql.performance.handler_read_next{$env,$customerid,$host} by {host} + avg:mysql.performance.handler_read_prev{$env,$customerid,$host} by {host}",
