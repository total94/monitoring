    },
    {
      "definition": {
        "viz": "timeseries",
        "requests": [
          {
            "q": "avg:mysql.performance.handler_read_rnd{$env,$customerid,$host} by {host}",
            "conditional_formats": [],
            "type": "area",
            "aggregator": "avg"
          }
        ]
      },
      "title": "Handler Read Random"
    },
    {
      "definition": {
        "viz": "timeseries",
        "requests": [
          {
            "q": "avg:mysql.innodb.row_lock_time{$env,$customerid,$host} by {host}",
